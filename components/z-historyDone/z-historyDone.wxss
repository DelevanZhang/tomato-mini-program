.z-history-wrapper {
  width: 710rpx;
  background-color: #FFFFFF;
  margin-left: auto;
  margin-right: auto;
  padding:20rpx;
}

.z-history-content {
  width: 504rpx;
  font-size: 28rpx;
  color: #333;
  flex-grow:1 ;
  margin-left: 40rpx;
}

.z-history-item {
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.z-history-historyTime{
  font-size: 36rpx;
  color:#333333;
}
.z-history-time{
  font-size: 28rpx;
  color:#333333;
}